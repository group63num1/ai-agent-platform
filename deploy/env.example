# ==================== 项目配置 ====================
# 项目名称（用于容器命名）
PROJECT_NAME=ai-agent

# 时区设置
TZ=Asia/Shanghai

# ==================== 数据库配置 ====================
# MySQL 配置
MYSQL_PORT=3306
MYSQL_DATABASE=demo_db
MYSQL_ROOT_PASSWORD=123456

# ==================== 后端服务配置 ====================
# Spring Boot 后端端口
BACKEND_PORT=8080

# Spring Boot 环境配置
SPRING_PROFILES_ACTIVE=prod

# Java 运行参数
JAVA_OPTS=-Xms512m -Xmx1g

# ==================== AI Agent 服务配置 ====================
# AI Agent 端口
AI_AGENT_PORT=8000

# AI Agent 调试模式
AI_AGENT_DEBUG=false

# AI Agent 日志级别
AI_AGENT_LOG_LEVEL=INFO

# AI Agent 数据库连接池配置
AI_AGENT_DB_POOL_SIZE=5
AI_AGENT_DB_MAX_OVERFLOW=10

# AI Agent 后端超时配置
AI_AGENT_BACKEND_TIMEOUT=30

# AI Agent RAG 配置
AI_AGENT_CHUNK_SIZE=1000
AI_AGENT_CHUNK_OVERLAP=200
AI_AGENT_RETRIEVAL_K=3

# AI Agent Agent 配置
AI_AGENT_MAX_ITERATIONS=5
AI_AGENT_MAX_HISTORY_LENGTH=20
AI_AGENT_SUMMARY_TRIGGER=16
AI_AGENT_VERBOSE=true

# AI Agent 插件配置
AI_AGENT_PLUGIN_TIMEOUT=30
AI_AGENT_PLUGIN_MAX_RETRIES=3

# AI Agent CORS 配置（多个用逗号分隔）
AI_AGENT_CORS_ORIGINS=*

# ==================== 前端服务配置 ====================
# 前端管理端端口
WEB_ADMIN_PORT=80

# Node.js 环境
NODE_ENV=production

# ==================== 数据库管理工具配置 ====================
# PHPMyAdmin 端口
PHPMYADMIN_PORT=8081

